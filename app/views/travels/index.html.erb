<div class="span3">
  <div class="span8">
    <div class="panel panel-default">
      <div id="map">

      </div>
      <!-- Default panel contents -->
        <table class="table table-condensed table-striped">
          <thead>
            <tr>
              <th>Start city</th>
              <th>End city</th>
              <th>Price</th>
              <th>Departure</th>
              <th>Arrival</th>
              <th>Direct trip</th>
              <th>Company</th>
              <th>Type</th>
              <th>Start airport</th>
              <th>End airport</th>
              <th>GÃ©oloc</th>
            </tr>
          </thead>

          <tbody>
            <% @travels.each do |travel| %>
              <tr>
                <td><%= travel.start_city %></td>
                <td class="city"><%= travel.end_city %></td>
                <td><%= travel.price %></td>
                <td><%= travel.departure %></td>
                <td><%= travel.arrival %></td>
                <td><%= travel.direct_trip %></td>
                <td><%= travel.company %></td>
                <td><%= travel.type %></td>
                <td><%= travel.start_airport %></td>
                <td><%= travel.end_airport %></td>
                <td class="coordinates"><%= travel.coordinates %></td>
              </tr>
            <% end %>
          </tbody>
        </table>
      </div>
  </div>

<% content_for(:more_js) do %>
    <script type="text/javascript">
        travelsJs.init();
        travelsJs.geoMap();

    </script>
<% end %>
